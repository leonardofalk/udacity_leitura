{
  "name": "udaciblog",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "g": "f() { hygen $1 new --name $2; }; f",
    "lint:fix": "eslint ./src --fix",
    "lint": "eslint ./src",
    "start": "react-app-rewired start",
    "start:dev": "concurrently -k -c green,cyan -n react,server -p name \"react-app-rewired start\" \"yarn install --cwd server/api-server && node server/api-server/server\"",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "react-app-rewired test --env=jsdom --coverage --colors --ci",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage --colors && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "antd": "^3.3.0",
    "apisauce": "^0.14.3",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0-alpha.2",
    "react-app-rewire-hot-loader": "^1.0.0",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.3.0-alpha.2",
    "react-masonry-component": "^6.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-eslint": "^8.2.2",
    "babel-plugin-import": "^1.6.7",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "hygen": "^1.5.1",
    "jest-enzyme": "^6.0.0",
    "react-hot-loader": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,mjs}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/__tests__/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTests.js",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/stories/**/*",
      "!src/registerServiceWorker.js"
    ]
  }
}
